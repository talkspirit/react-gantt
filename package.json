{
  "name": "@talkspirit/react-gantt",
  "version": "2.5.2",
  "description": "A customizable, high-performance React Gantt chart component for building project timelines.",
  "productTag": "gantt",
  "productTrial": true,
  "type": "module",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "react-gantt",
    "gantt-react",
    "gantt",
    "gantt-chart",
    "react-gantt-chart",
    "gantt-chart-component",
    "gantt-diagram",
    "react-gantt-component",
    "svar widgets",
    "svar-react",
    "timeline",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "build:demo": "BUILD_DEMOS=true vite build",
    "build:full-css": "BUILD_FULL_CSS=true vite build",
    "lint": "eslint . --report-unused-disable-directives",
    "preview": "vite preview",
    "prepare": "husky",
    "prettier": "prettier --write ./src/ ./demos/"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./style.css": "./dist/index.css",
    "./all.css": "./dist-full/index.css"
  },
  "types": "./types/index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "sideEffects": false,
  "files": [
    "dist",
    "types",
    "licenses",
    "package.json",
    "dist-full/index.css"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@svar-ui/gantt-data-provider": "2.5.1",
    "@svar-ui/gantt-locales": "2.5.1",
    "@svar-ui/gantt-store": "2.5.1",
    "@svar-ui/lib-dom": "0.11.1",
    "@svar-ui/lib-state": "1.9.6",
    "@svar-ui/lib-react": "1.2.2",
    "@svar-ui/react-core": "2.4.3",
    "@svar-ui/react-menu": "2.4.3",
    "@svar-ui/react-grid": "2.5.1",
    "@svar-ui/react-toolbar": "2.4.3",
    "@svar-ui/react-editor": "2.4.3"
  },
  "homepage": "https://svar.dev/react/gantt/",
  "repository": {
    "type": "git",
    "url": "https://github.com/svar-widgets/react-gantt.git"
  },
  "bugs": {
    "url": "https://forum.svar.dev"
  },
  "devDependencies": {
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.35.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "7.8.2",
    "react-router-dom": "7.8.2",
    "vite": "7.1.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,js,svelte}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
